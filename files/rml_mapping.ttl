## Case-Event Relation Mapping (FJE-OK)
map:case_event rdf:type rr:TriplesMap ;
    rdfs:label "case_event" ;
    rml:logicalSource [
            rml:iterator "$.event_object[?(@.qualifier==\"case_event\")]" ;
            rml:referenceFormulation ql:JSONPath ;
            rdf:type rml:LogicalSource 
        ] ;
    
    rr:subjectMap [
        rr:template "https://w3id.org/ocedo/resource/case_event-{objectID}-{eventID}" 
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdf:type ;
        rr:objectMap [
			rr:constant ocedo:CaseEvent ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate ocedo:ce_case ;
        rr:objectMap [
			rr:template "https://w3id.org/ocedo/resource/object-{objectID}" ;
            rr:termType rr:IRI ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate ocedo:ce_event ;
        rr:objectMap [
			rr:template "https://w3id.org/ocedo/resource/event-{eventID}" ;
            rr:termType rr:IRI ;
        ] ;
    ] .

